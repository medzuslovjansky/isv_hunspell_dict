#!/usr/bin/env bash

copyDictionary() {
  local DICT_PATH="../hunspell/output/dictionaries"
  cp "$DICT_PATH/$1.aff" $2.aff
  cp "$DICT_PATH/$1.dic" $2.dic
}

copyDictionary Medzuslovjansky_KomboLatinicaKirilica art-x-interslv
copyDictionary Medzuslovjansky_Kirilica art-Cyrl-x-interslv
copyDictionary Medzuslovjansky_LatinicaStandard art-Latn-x-interslv
copyDictionary Medzuslovjansky_LatinicaEtimologicna art-Latn-x-interslv-etymolog
